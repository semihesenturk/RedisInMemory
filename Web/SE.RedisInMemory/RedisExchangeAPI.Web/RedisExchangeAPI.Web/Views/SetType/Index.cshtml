@model HashSet<string>

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name"/>
    <button type="submit">Add</button>
</form>

@foreach (var item in Model)
{
    <h2>@item 
        <a asp-controller="SetType" 
           asp-action="DeleteItem" 
           asp-route-name="@item">Delete</a></h2>
}